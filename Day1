Q2)

<script>
      <h1>Primitive Data Types in JavaScript</h1>
    <button onclick="primitiveDataTypesDemo()">Primitive Data Types</button>
    <script src="app.js"></script>

    </script>

function primitiveDataTypesDemo() {
   
    let age = 18; 
    let name = "kiran"; 
    let setStatus = false;
    let userDefault = undefined; 
    let responseValue = null;

    
    console.log("Initial values:");
    console.log("age:", age);
    console.log("name:", name);
    console.log("setStatus:", setStatus);
    console.log("userDefault:", userDefault);
    console.log("responseValue:", responseValue);

       age = 25;
    let oldage = age;
    let status = setStatus;
    setStatus = true;

        console.log("Updated values:");
    console.log("age:", age); // 25
    console.log("oldage:", oldage); // 25
    console.log("setStatus:", setStatus); // true
    console.log("status:", status); // false

    
    alert("Check the console output!");
}

import React from 'react';

function App() {
    return (
        <div className="App">
            {/* some default code */}
            <p>Primitive Data Types copy the values</p>
            <button onClick={() => primitiveDataTypesDemo()}>Primitive Data Types</button>
        </div>
    );
}

export default App;







