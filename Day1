 Q1) <script>
      function alertmsg() {
        alert("Message from Javascript Alert!");
        console.log("Message to developer");
      }
    </script>

import React from 'react';

function App() {


    return (
        <div>
            <h1>Let us see the output of JAVASCRIPT</h1>
            <button onClick={alertmsg}>click</button>
        </div>
    );
}

export default App;

Q2)
 <script>
    function referenceDataTypesDemo() {
    let myobject = { name: "Madras" };
    let newObject = myobject; 
    myobject.name = "Chennai";
    let myarray = ["a", "e", "i", "o"];
    let vowelArray = myarray;
    myarray.push("u");
    console.log("newObject.name:", newObject.name); 
    console.log("vowelArray:", vowelArray); 
    alert("Check the console output!");
}
    </script>
import React from 'react';
function App() {
    return (
        <div>
            <h1>Reference Data Types in JavaScript</h1>
            <button onClick={referenceDataTypesDemo}>Reference Data Types</button>
        </div>
    );
}

export default App;

